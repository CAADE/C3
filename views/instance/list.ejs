<div class="gadget" style="width:calc(100% - 15px);height:100%;background:white;">
  <div class="gadget-header">Application Instances<span style="float: right;"><%= instances.length %></span>
  </div>
  <div style="overflow-y:auto;position:relative;max-height:50%;height:auto;background:white;">
    <%- partial("../partials/appInstanceList", {instances:instances}); %>
  </div>
  <hr>
  <div id="graph" style="overflow-y:auto;height:500px;">
    <%- partial('../partials/instanceGraph', {height: 500, width: 1000, query: "/instance/list?mode=json"}) %>
  </div>
</div>
