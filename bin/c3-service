#!/usr/bin/env node

var program =  require('commander');

program
  .version("0.2.0")
  .command('create <serviceName>', 'Create a Service')
  .command('ls', 'List Services')
  .command('remove <serviceName>', 'Remove a Service')
  .command('show <serviceName>', 'Show details about a Service')
  .command('add-script <serviceName>', 'Add script for service')
  .command('update <serviceName>', 'Update Service information')
  .parse(process.argv);
