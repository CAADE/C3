../../bin/caade environment create Local
../../bin/caade environment create Dev
../../bin/caade service create Redis --env Local
../../bin/caade service create Mongo --env Local
../../bin/caade service create NodeJS --env Local
../../bin/caade service add-script Redis --env Local --event=Install --filename=redisInstall.sh
../../bin/caade service add-script Redis --env Local --event=Shutdown --filename=redisShutdown.sh
../../bin/caade service create Redis --env Dev
../../bin/caade service create Mongo --env Dev
../../bin/caade service create NodeJS --env Dev
../../bin/caade service add-script Redis --env Dev --event=Install --filename=redisInstall.sh
../../bin/caade service add-script Redis --env Dev --event=Shutdown --filename=redisShutdown.sh
../../bin/caade stack create NodeJS --env=Local --filename=stackDef.yaml
../../bin/caade stack create NodeJS --env=Dev --filename=stackDef.yaml
../../bin/caade app create Mine --stack=NodeJS --filename=caade-app.yaml
