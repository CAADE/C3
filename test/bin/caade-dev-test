../../bin/c3 environment create Local
../../bin/c3 environment create Dev
../../bin/c3 service create Redis --env Local
../../bin/c3 service create Mongo --env Local
../../bin/c3 service create NodeJS --env Local
../../bin/c3 service add-script Redis --env Local --event=Install --filename=redisInstall.sh
../../bin/c3 service add-script Redis --env Local --event=Shutdown --filename=redisShutdown.sh
../../bin/c3 service create Redis --env Dev
../../bin/c3 service create Mongo --env Dev
../../bin/c3 service create NodeJS --env Dev
../../bin/c3 service add-script Redis --env Dev --event=Install --filename=redisInstall.sh
../../bin/c3 service add-script Redis --env Dev --event=Shutdown --filename=redisShutdown.sh
../../bin/c3 stack create NodeJS --env=Local --filename=stackDef.yaml
../../bin/c3 stack create NodeJS --env=Dev --filename=stackDef.yaml
../../bin/c3 app create Mine --stack=NodeJS --filename=c3-app.yaml
../../bin/c3 up
../../bin/c3 up --env=Dev
../../bin/c3 ps
../../bin/c3 kill
../../bin/c3 kill --env=Dev
../../bin/c3 ps
