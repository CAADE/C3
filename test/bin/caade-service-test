../../bin/c3 environment create Local
../../bin/c3 service create Redis --env Local
../../bin/c3 service create Mongo --env Local
../../bin/c3 service create NodeJS --env Local
../../bin/c3 service add-script Redis --env Local --event=Install --filename=redisInstall.sh
../../bin/c3 service add-script Redis --env Local --event=Shutdown --filename=redisShutdown.sh
../../bin/c3 stack create NodeJS --env Local --filename stackDef.yaml
../../bin/c3 app create Mine --stack NodeJS
../../bin/c3 up
../../bin/c3 ps
../../bin/c3 kill Mine-web
